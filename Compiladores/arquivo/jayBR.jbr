
VAI

!valor1 @inteiro.
!valor2 @inteiro.
!multiplicacao @real.

INSIRA "Digite o primeiro valor: ".
RECEBA !valor1.

INSIRA "Digite o segundo valor: ".
RECEBA !valor2.

!multiplicacao <- !valor1 $* !valor2.


CONDICAO [!multiplicacao $> 0]{
	INSIRA "Primeiro maior que segundo".
}CONTRA{
	INSIRA "Segundo maior que primeiro".
}

CABEI

UEAUHA